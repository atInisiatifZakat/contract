{
    "name": "inisiatif/contract",
    "description": "Contract package using in Inisiatif Zakat Indonesia",
    "license": "proprietary",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/cache": "^9.19|^10.10",
        "illuminate/contracts": "^9.19|^10.10"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Inisiatif\\Package\\Contract\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
